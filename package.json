{
  "name": "consumer-pgmq",
  "version": "1.0.0",
  "description": "The consumer of Supabase pgmq",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start:example:postgres": "ts-node ./examples/consumerPostgresDriver.ts",
    "start:example:supabase": "ts-node ./examples/consumerSupabaseDriver.ts",
    "start:dev": "ts-node-dev ./src/index.ts",
    "test": "jest ./tests/*.spec.ts --detectOpenHandles",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "supabase",
    "pgmq",
    "consumer"
  ],
  "author": "Tiago Rosa da costa<tiagorosadacosta@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.7.1",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "dotenv": "^17.2.1",
    "jest": "^30.1.1",
    "knex": "^3.1.0",
    "pg": "^8.16.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}